<h1>Posts</h1>
<%= render 'form' %>

<ul>
  <% for post in @posts %>
  <li id="post_<%= post.id %>">
  	<%= "Title: #{post.title}" %> 
  	</br>
  	<%= "Content: #{post.content}" %> 
  	</br>
  	<%= link_to 'Edit', edit_post_path(post) %>
  	<%= link_to 'Delete', post, method: :delete %>
  </li>
  <% end %>
</ul>